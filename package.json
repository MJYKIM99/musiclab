{
  "name": "ocean-loop-project",
  "version": "1.1.0",
  "description": "An interactive audio-visual experiment exploring particle systems and generative art",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "npx http-server -p 8000 -o",
    "serve": "python3 -m http.server 8000",
    "dev": "npx http-server -p 8000 -c-1 --cors",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint **/*.js --fix",
    "format": "prettier --write \"**/*.{js,html,css,json,md}\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "p5js",
    "generative-art",
    "audio-visual",
    "interactive",
    "particles",
    "creative-coding",
    "canvas",
    "webgl"
  ],
  "author": "yijiayidesign",
  "license": "MIT",
  "devDependencies": {
    "@types/p5": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
